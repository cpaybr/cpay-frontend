{
  "name": "sdk-finance",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build --mode",
    "lint": "vue-cli-service lint --debug",
    "lint:fix": "vue-cli-service lint --debug --fix",
    "lint:scss": "stylelint **/*.{vue,html,scss,sass} --fix",
    "test:unit": "vue-cli-service test:unit",
    "storybook": "start-storybook -c .storybook -s ./public",
    "audit:fix": "npm audit fix",
    "build:storybook": "build-storybook -c .storybook -o public/storybook  -s public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx|vue)": [
      "npm run lint:fix",
      "npm run lint:scss"
    ],
    "*.(json)": [
      "npm run lint:fix"
    ]
  },
  "dependencies": {
    "axios": "^0.21.4",
    "chart.js": "^3.7.0",
    "core-js": "^3.20.3",
    "credit-card-type": "^9.1.0",
    "dayjs": "^1.10.7",
    "element-ui": "^2.15.8",
    "file-saver": "^2.0.2",
    "gmap-vue": "^1.5.0",
    "lodash": "^4.17.20",
    "normalize.css": "^8.0.1",
    "numbro": "^2.3.6",
    "swiper": "^6.8.4",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.3",
    "vue-i18n": "^8.27.0",
    "vue-property-decorator": "^8.4.2",
    "vue-resize-directive": "^1.2.0",
    "vue-router": "^3.5.3",
    "vue-the-mask": "^0.11.1",
    "vue2-daterange-picker": "^0.6.8",
    "vuex": "^3.4.0",
    "vuex-module-decorators": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/vue": "^6.4.18",
    "@types/googlemaps": "^3.43.3",
    "@types/jest": "^24.0.19",
    "@types/lodash": "^4.14.178",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-plugin-typescript": "^4.5.15",
    "@vue/cli-plugin-unit-jest": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^1.3.0",
    "autoprefixer": "^9.0.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-component": "^1.1.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-storybook": "^0.5.6",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "postcss": "^8.4.5",
    "postcss-loader": "^4.3.0",
    "react-is": "^16.13.1",
    "sass": "^1.54.3",
